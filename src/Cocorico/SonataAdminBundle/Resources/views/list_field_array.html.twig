{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {%- if value is empty -%}
        &nbsp;
    {%- else -%}
        {% for item in value %}
            {% if loop.index0 > 0 %},{% endif %}
            {{ item|trans({}, field_description.options.data_trans) }}
        {% endfor %}
    {%- endif -%}
{% endblock %}
